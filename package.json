{
  "name": "aisup-widget",
  "version": "1.0.0",
  "description": "Embeddable AI Support Chat Widget with React/Next.js support",
  "type": "module",
  "main": "dist/widget.umd.js",
  "module": "dist/widget.esm.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/widget.esm.js",
      "require": "./dist/widget.umd.js",
      "types": "./dist/types/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.umd.js",
      "types": "./dist/types/react.d.ts"
    },
    "./widget.css": "./dist/widget.css",
    "./iife": "./dist/widget.iife.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist/types",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "chat",
    "widget",
    "support",
    "ai",
    "embed",
    "react",
    "nextjs",
    "customer-support",
    "live-chat",
    "chatbot"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "socket.io-client": "^4.7.5"
  }
}
